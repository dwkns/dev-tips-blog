{
  "name": "notion-to-11ty",
  "version": "0.0.1",
  "main": "index.js",
  "license": "MIT",
  "dependencies": {
    "@11ty/eleventy": "^3.0.0",
    "@notionhq/client": "^2.2.15",
    "@tailwindcss/typography": "^0.5.15",
    "eleventy-plugin-console-plus": "^0.0.4",
    "markdown-it-class": "^1.0.0",
    "notion-to-md": "^3.1.1",
    "npm-run-all": "^4.1.5",
    "rimraf": "^6.0.1",
    "tailwindcss": "^3.4.13"
  },
  "type": "module",
  "engines" : { 
    "node" : ">=20.0.0"
  },
  "scripts": {
    "clean": "rimraf dist build",
    "dev": "npm-run-all clean -p dev:* ",
    "dev:tailwind": "tailwind -i ./src/styles/tailwind.css -o ./src/styles/main.css --watch",
    "dev:eleventy": "eleventy --serve --quiet",
    "build": "npm-run-all clean -p build:* ",
    "build:tailwind": "tailwind -i ./src/styles/tailwind.css -o ./src/styles/main.css ",
    "build:eleventy": "eleventy"
  },
  "author": "dazza"
}
